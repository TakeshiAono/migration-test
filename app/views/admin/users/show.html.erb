<h1>admin_userの詳細</h1>
<p><%= link_to '一覧', admin_users_path %></p>
<tbody>
<tr>
<th><%= 管理者名 %></th>
<td><%= @user.id %></td>
</tr>
<tr>
<th><%= 名前 %></th>
<td><%= @user.name %></td>
</tr>
<tr>
<th><%= email %></th>
<td><%= @user.email %></td>
</tr>
<tr>
<th><%= admin %></th>
<td><%= @user.admin? ? 'あり': 'なし' %></td>
</tr>
<tr>
<th><%= 新規作成 %></th>
<td><%= @user.create_at%></td>
</tr>
<tr>
<th><%= 編集日 %></th>
<td><%= @user.update_at%></td>
</tr>

<td><%= link_to t('view.edit'), edit_admin_user_path(user) %></td>
<td><%= link_to t('view.destroy'),[:admin, user], method: :delete, data: { confirm: t('view.are_you_sure?') } %></td>
